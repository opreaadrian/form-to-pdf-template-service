{
  "env": {
    "node": true,
    "mocha": true,
    "browser": true,
    "amd": true,
    "jquery": true,
    "jasmine": true,
    "protractor": true,
    "es6": true
  },
  "parserOptions": {
    "sourceType": "module"
  },
  "globals": {
    "_": true,
    "Switchery": true
  },
  "rules": {
    "no-trailing-spaces": [2, { "skipBlankLines": false }],
    "eol-last": 2,
    "no-empty": 2,
    "no-mixed-spaces-and-tabs": 2,
    "linebreak-style": [2, "unix"],
    "no-multi-str": 2,
    "key-spacing": [2, {
      "beforeColon": false,
      "afterColon": true
    }],
    "space-unary-ops": [1, {
      "words": true,
      "nonwords": false
    }],
    "no-spaced-func": 2,
    "space-infix-ops": [2, {"int32Hint": false}],
    "keyword-spacing": [2, {before: true}],
    "space-before-blocks": [2, "always"],
    "space-before-function-paren": [2, "never"],
    "one-var": [2, {
      "var": "never",
      "let": "never",
      "const": "never"
    }],
    "no-shadow-restricted-names": 2,
    "no-shadow": [2, {"hoist": "all"}],
    "no-catch-shadow": [2],
    "no-irregular-whitespace": 2,
    "strict": [2, "global"],
    "no-eq-null": 2,
    "no-use-before-define": 2,
    "no-unused-vars": [2, {"vars": "all", "args": "after-used"}],
    "no-undef": 2,
    "no-undef-init": 2,
    "dot-notation": [2, {"allowKeywords": true, "allowPattern": ""}],
    "dot-location": [2, "property"],
    "no-caller": 2,
    "eqeqeq": 2,
    "quotes": [2, "single"],
    "no-mixed-requires": [2, false],
    "no-underscore-dangle": 2,
    "yoda": [2, "never", {
      "exceptRange": false,
      "onlyEquality": false
    }],
    "indent": [2, 2],
    "brace-style": [2, "1tbs"],
    "comma-style": [2, "last", {"exceptions": {"ArrayExpression": false, "ObjectExpression": true, "VariableDeclaration": true} }],
    "default-case": 2,
    "guard-for-in": 2,
    "no-floating-decimal": 2,
    "no-nested-ternary": 2,
    "no-undefined": 2,
    "no-unused-expressions": 2,
    "radix": 2,
    "wrap-iife": [2, "inside"],
    "camelcase": [2, {"properties": "always"}],
    "max-len": [2, 100, 4, {"ignoreComments": true, "ignoreUrls": true}],
    "new-cap": [2, {
      "newIsCap": true,
      "capIsNew": false
    }],
    "no-label-var": 2
  }
}
